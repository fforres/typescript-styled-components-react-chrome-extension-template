{
  "name": "react-typescript-chrome-extension-starter",
  "version": "1.0.0",
  "description": "A Chrome template, built with React, TypeScript, Styled Components,  EsLint & Prettier",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack -w --config webpack.dev.js",
    "test": "jest --config=jest.config.js",
    "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
    "lint:css": "stylelint './src/**/*'",
    "prettify": "prettier --write \"src/**/*.ts*\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fforres/typescript-styled-components-react-chrome-extension-template.git"
  },
  "keywords": [
    "chrome",
    "react",
    "typescript",
    "boilerplate",
    "extension"
  ],
  "author": "@fforres",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fforres/typescript-styled-components-react-chrome-extension-template/issues"
  },
  "homepage": "https://github.com/fforres/typescript-styled-components-react-chrome-extension-template#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.2.8",
    "@types/chrome": "^0.0.91",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
